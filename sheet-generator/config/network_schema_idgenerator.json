{
	"type": "object",
	"properties": {
	    "network_version": {
	        "type": "integer",
	        "minimum": 0,
	        "maximum": 255
	    },
		"messages": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string",
						"pattern": "^[A-Z][A-Z0-9_]*$"
					},
					"topic": {
						"type": "string",
						"pattern": "^[A-Za-z][A-Za-z0-9_]*$"
					},
					"priority": {
						"type": "integer",
						"minimum": 0,
						"maximum": 7
					}
				},
				"required": ["name", "topic", "priority"],
                "additionalProperties": true
			}
		}
	},
    "additionalProperties": false
}